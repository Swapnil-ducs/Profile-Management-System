<h1>Search Result<h1>
 <table>
 <tr>
      <td>id </td>
      <td> parent_id </td>
      <td> user_name </td>
      <td>user_email </td>
 </tr>	
<% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.parent_id %></td>
      <td><%= user.user_name %></td>
      <td><%= user.user_email %></td>
      <td><%= link_to 'Edit', admin_edit_path(user.id) %></td>
    </tr>
 </table>
  <% end %>